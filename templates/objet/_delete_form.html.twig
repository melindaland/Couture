<form method="post" action="{{ path('app_objet_delete', {'id': objet.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet objet ?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ objet.id) }}">
    <button class="btn btn-outline-danger btn-sm" title="Supprimer">
        <i class="bi bi-trash3"></i>
    </button>
</form>
